                                <local:TableButton Tag="102"/>
                                <local:TableButton Tag="103"/>
                            </StackPanel>
                        </Grid>
                    </Viewbox>
                </Grid>
            </TabItem>
            <TabItem x:Name="inorganicsPage">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <Grid Height="35">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="1*"/>
                            <ColumnDefinition Width="1*"/>
                        </Grid.ColumnDefinitions>
                        <TextBox x:Name="searchBox" Style="{DynamicResource searchBoxStyle}"/>
                    </Grid>
                    <DataGrid x:Name="inorganicGrid" ItemsSource="{Binding Text, ElementName=searchBox, Converter={StaticResource searchEngine}, ConverterParameter=inorganic}" Grid.Row="1" Style="{DynamicResource labGrid}">

                        <DataGrid.Columns>
                            <DataGridTextColumn Header="Name" SortDirection="Descending" SortMemberPath="Name" IsReadOnly="True" Binding="{Binding Name}"/>
                            <DataGridTemplateColumn Header="Formula" SortMemberPath="FormulaString" IsReadOnly="True">
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <DataTemplate.Resources>
                                            <Style TargetType="{x:Type TextBlock}">
                                                <Setter Property="FontFamily" Value="Calibri"/>
                                            </Style>
                                        </DataTemplate.Resources>
                                        <Label Content="{Binding Formula, Converter={StaticResource formulaDisplayConverter}, Mode=OneWay}" Foreground="{Binding Foreground, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type DataGridCell}}}"/>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                            <DataGridTextColumn Header="State" SortMemberPath="State" IsReadOnly="True" Binding="{Binding State}"/>
                            <DataGridTextColumn Header="Appearance" SortMemberPath="Appearance" IsReadOnly="True" Binding="{Binding Appearance}"/>
                            <DataGridTextColumn Header="Molar Mass" SortMemberPath="MolarMass" IsReadOnly="True" Binding="{Binding MolarMass, StringFormat=\{0\} g/mol}"/>
                            <DataGridTextColumn Header="Density" SortMemberPath="Density" IsReadOnly="True" Binding="{Binding Density, StringFormat=\{0\} g/mL, TargetNullValue=No data}"/>
                            <DataGridTextColumn Header="Melting Point" SortMemberPath="MeltingPoint" IsReadOnly="True" Binding="{Binding MeltingPoint, StringFormat=\{0\} K, TargetNullValue=No data}"/>
                            <DataGridTextColumn Header="Boiling Point" SortMemberPath="BoilingPoint" IsReadOnly="True" Binding="{Binding BoilingPoint, StringFormat=\{0\} K, TargetNullValue=No data}"/>
                            <DataGridTextColumn Header="Solubility" SortMemberPath="Solubility" IsReadOnly="True" Binding="{Binding Solubility}"/>
                        </DataGrid.Columns>
                    </DataGrid>
                </Grid>
            </TabItem>
            <TabItem x:Name="organicsPage">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <Grid Height="35">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="1*"/>
                            <ColumnDefinition Width="1*"/>
                        </Grid.ColumnDefinitions>
                        <TextBox x:Name="organicSearchBox" Style="{DynamicResource searchBoxStyle}"/>
                    </Grid>
                    <DataGrid x:Name="organicGrid" ItemsSource="{Binding Text, ElementName=organicSearchBox, Converter={StaticResource searchEngine}, ConverterParameter=organic}" Grid.Row="1" Style="{DynamicResource labGrid}">

                        <DataGrid.Columns>
                            <DataGridTextColumn Header="Name" SortDirection="Descending" SortMemberPath="Name" IsReadOnly="True" Binding="{Binding Name}"/>
                            <DataGridTemplateColumn Header="Formula" SortMemberPath="FormulaString" IsReadOnly="True">
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <DataTemplate.Resources>
                                            <Style TargetType="{x:Type TextBlock}">
                                                <Setter Property="FontFamily" Value="Calibri"/>
                                            </Style>
                                        </DataTemplate.Resources>
                                        <Label Content="{Binding Formula, Converter={StaticResource formulaDisplayConverter}, Mode=OneWay}" Foreground="{Binding Foreground, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type DataGridCell}}}"/>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                            <DataGridTextColumn Header="State" SortMemberPath="State" IsReadOnly="True" Binding="{Binding State}"/>
                            <DataGridTextColumn Header="Appearance" SortMemberPath="Appearance" IsReadOnly="True" Binding="{Binding Appearance}"/>
                            <DataGridTextColumn Header="Molar Mass" SortMemberPath="MolarMass" IsReadOnly="True" Binding="{Binding MolarMass, StringFormat=\{0\} g/mol}"/>
                            <DataGridTextColumn Header="Density" SortMemberPath="Density" IsReadOnly="True" Binding="{Binding Density, StringFormat=\{0\} g/mL, TargetNullValue=No data}"/>
                            <DataGridTextColumn Header="Melting Point" SortMemberPath="MeltingPoint" IsReadOnly="True" Binding="{Binding MeltingPoint, StringFormat=\{0\} K, TargetNullValue=No data}"/>
                            <DataGridTextColumn Header="Boiling Point" SortMemberPath="BoilingPoint" IsReadOnly="True" Binding="{Binding BoilingPoint, StringFormat=\{0\} K, TargetNullValue=No data}"/>
                            <DataGridTextColumn Header="Solubility" SortMemberPath="Solubility" IsReadOnly="True" Binding="{Binding Solubility}"/>
                        </DataGrid.Columns>
                    </DataGrid>
                </Grid>
            </TabItem>
            <TabItem x:Name="reactionsPage">
                <Grid>
                    <Grid x:Name="searchReactionGrid">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>
                        <StackPanel>
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition/>
                                    <ColumnDefinition Width="Auto"/>
                                </Grid.ColumnDefinitions>
                                <TextBox x:Name="reactionsSearchBox" FontSize="20" Style="{DynamicResource searchBoxStyle}" VerticalAlignment="Top"/>
                                <ComboBox x:Name="reactionTypeComboBox" Grid.Column="1" Style="{DynamicResource labComboBoxStyle}" Foreground="#FFD8D8D8" Width="150" Background="#FF2A2A2A" Margin="0,5,5,5" SelectedIndex="0">
                                    <ComboBoxItem Content="All types"/>
                                    <ComboBoxItem Content="Decomposition"/>
                                    <ComboBoxItem Content="Electrolysis"/>
                                    <ComboBoxItem Content="Synthesis"/>
                                    <ComboBoxItem Content="Other"/>
                                </ComboBox>
                            </Grid>
                            <Label x:Name="reactionsStatusText" Content="Search for a reaction. Use the negate symbol(-) for all reactions." FontSize="22" Foreground="#FFC5C5C5" IsHitTestVisible="False" VerticalAlignment="Top" HorizontalContentAlignment="Center" Padding="5,0,5,5">
                                <Label.Effect>
                                    <DropShadowEffect ShadowDepth="1" BlurRadius="3"/>
                                </Label.Effect>
                            </Label>
                        </StackPanel>
                        <local:ReactionList x:Name="reactionList" Foreground="White" BorderBrush="LimeGreen" BorderThickness="0,1,0,0" Background="#FF4B4B4B" Grid.Row="1"/>
                    </Grid>
                    <Grid x:Name="selectedReactionGrid" Visibility="Hidden" Opacity="0">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition/>
                        </Grid.ColumnDefinitions>
                        <Grid.Resources>
                            <Style x:Key="subtitleStyle" TargetType="Label">
                                <Setter Property="FontFamily" Value="{StaticResource NevisFont}"/>
                                <Setter Property="Foreground" Value="White"/>
                                <Setter Property="FontSize" Value="19"/>
                                <Setter Property="BorderBrush" Value="LightGray"/>
                                <Setter Property="BorderThickness" Value="0,0,0,1"/>
                                <Setter Property="HorizontalAlignment" Value="Left"/>
                                <Setter Property="Padding" Value="5,2"/>
                            </Style>
                            <Style x:Key="descriptionStyle" TargetType="Label">
                                <Setter Property="Foreground" Value="#FFE2E2E2"/>
                                <Setter Property="FontSize" Value="18"/>
                                <Setter Property="BorderBrush" Value="LightGray"/>
                                <Setter Property="BorderThickness" Value="1,0,0,0"/>
                                <Setter Property="HorizontalAlignment" Value="Left"/>
                                <Setter Property="Padding" Value="5,2"/>
                                <Setter Property="Margin" Value="0,0,0,10"/>
                            </Style>
                            <Style x:Key="subtitleRightStyle" TargetType="Label" BasedOn="{StaticResource subtitleStyle}">
                                <Setter Property="HorizontalAlignment" Value="Right"/>
                            </Style>
                            <Style x:Key="descriptionRightStyle" TargetType="Label" BasedOn="{StaticResource descriptionStyle}">
                                <Setter Property="BorderThickness" Value="0,0,1,0"/>
                                <Setter Property="HorizontalAlignment" Value="Right"/>
                            </Style>
                            <Style x:Key="listStyle" TargetType="ScrollViewer" BasedOn="{StaticResource labScrollViewerStyle}">
                                <Setter Property="VerticalScrollBarVisibility" Value="Auto"/>
                                <Setter Property="Margin" Value="5"/>
                                <Setter Property="Background" Value="#FF292929"/>
                            </Style>
                        </Grid.Resources>
                        <StackPanel Grid.ColumnSpan="3" Margin="5">
                            <local:ReactionRow x:Name="reactionSelectedRow" Foreground="White" HorizontalContentAlignment="Center" FontSize="45" Height="70"/>
                            <Grid>
                                <StackPanel>
                                    <Label Content="TYPE" Style="{DynamicResource subtitleStyle}"/>
                                    <Label x:Name="reactionTypeLabel"  Style="{DynamicResource descriptionStyle}"/>
                                    <Label Content="COMMENT" Style="{DynamicResource subtitleStyle}"/>
                                    <Label x:Name="reactionCommentLabel"  Style="{DynamicResource descriptionStyle}"/>
                                </StackPanel>
                                <StackPanel HorizontalAlignment="Right">
                                    <Label Content="MINIMUM TEMP." Style="{DynamicResource subtitleRightStyle}"/>
                                    <Label x:Name="reactionMinLabel"  Style="{DynamicResource descriptionRightStyle}"/>
                                    <Label Content="MAXIMUM TEMP." Style="{DynamicResource subtitleRightStyle}"/>
                                    <Label x:Name="reactionMaxLabel"  Style="{DynamicResource descriptionRightStyle}"/>
                                </StackPanel>
                            </Grid>
                        </StackPanel>
                        <ScrollViewer Grid.Row="1" Style="{DynamicResource listStyle}">
                            <StackPanel x:Name="reactantsPanel">

                            </StackPanel>
                        </ScrollViewer>
                        <Rectangle x:Name="arrowRect" Grid.Row="1" Grid.Column="1" Height="30" VerticalAlignment="Center" Width="50" Margin="10"/>
                        <ScrollViewer Grid.Column="2" Grid.Row="1" Style="{DynamicResource listStyle}">
                            <StackPanel x:Name="productsPanel">

                            </StackPanel>
                        </ScrollViewer>
                        <local:ImageButton x:Name="backToSearchBtn" Grid.Row="2" Margin="5" Grid.Column="2" VerticalAlignment="Bottom" HorizontalAlignment="Right" Background="#FF1D1D1D" Foreground="{DynamicResource labBlueBrush}" HorizontalTextAlignment="Center" Text="BACK" ReserveImageSpace="False" TextPadding="25,5" FontFamily="{DynamicResource NevisFont}"/>
                    </Grid>
                </Grid>
            </TabItem>
            <TabItem x:Name="inventoryPage">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <Grid Height="35">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="1*"/>
                            <ColumnDefinition Width="1*"/>
                        </Grid.ColumnDefinitions>
                        <TextBox x:Name="inventorySearchBox" Style="{DynamicResource searchBoxStyle}"/>
                    </Grid>
                    <DataGrid x:Name="inventoryGrid" ItemsSource="{Binding Text, ElementName=inventorySearchBox, Converter={StaticResource searchEngine}, ConverterParameter=inventory}" Grid.Row="1" Style="{DynamicResource labGrid}">
                        <DataGrid.Resources>
                            <local:MassToStringConverter x:Key="massConverter"/>
                        </DataGrid.Resources>
                        <DataGrid.Columns>
                            <DataGridTextColumn Header="Name" SortDirection="Descending" SortMemberPath="Name" IsReadOnly="True" Binding="{Binding Name}"/>
                            <DataGridTemplateColumn Header="Formula" SortMemberPath="FormulaString" IsReadOnly="True">
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <DataTemplate.Resources>
                                            <Style TargetType="{x:Type TextBlock}">
                                                <Setter Property="FontFamily" Value="Calibri"/>
                                            </Style>
                                        </DataTemplate.Resources>
                                        <Label Content="{Binding Formula, Converter={StaticResource formulaDisplayConverter}, Mode=OneWay}" Foreground="{Binding Foreground, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type DataGridCell}}}"/>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                            <DataGridTextColumn Header="Mass" SortMemberPath="Mass" IsReadOnly="True" Binding="{Binding Mass, Converter={StaticResource massConverter}}"/>
                            <DataGridTextColumn Header="Comment" Width="*" SortMemberPath="Appearance" IsReadOnly="True" Binding="{Binding Comment}"/>
                        </DataGrid.Columns>
                    </DataGrid>
                </Grid>
            </TabItem>
            <TabItem x:Name="calculatorTabPage">
                <TabItem.Resources>
                    <SolidColorBrush x:Key="borderBrush" Color="#FFAAA4A4"/>
                    <SolidColorBrush x:Key="textBrush" Color="#FFF1F1F1"/>
                    <SolidColorBrush x:Key="unitBrush" Color="#FF858585"/>
                    <Style TargetType="local:NumberTextBox">
                        <Setter Property="Background" Value="#FF323232"/>
                        <Setter Property="BorderBrush" Value="#FF939393"/>
                        <Setter Property="Width" Value="70"/>
                        <Setter Property="Foreground" Value="#FF76D476"/>
                        <Setter Property="TextAlignment" Value="Right"/>
                        <Setter Property="VerticalAlignment" Value="Top"/>
                    </Style>
                </TabItem.Resources>
                <StackPanel Margin="10">
                   
                    <Label Content="SOLUTIONS" Background="{x:Null}" Foreground="#FFF0F0F0" FontFamily="{DynamicResource NevisFont}" FontSize="20" TextOptions.TextFormattingMode="Display" BorderThickness="0,0,0,1" BorderBrush="{DynamicResource borderBrush}" HorizontalAlignment="Left" Padding="0"/>
                    <Border BorderBrush="{DynamicResource borderBrush}" BorderThickness="1,0,0,0">
                        <StackPanel HorizontalAlignment="Left">
                            <TextBlock Text="Molar and mass concentration convertion:" Foreground="#FFB0B0B0" Margin="5" FontSize="18.667"/>
                            <StackPanel Orientation="Vertical" HorizontalAlignment="Left">
                                <StackPanel Orientation="Horizontal" Margin="10,0,0,0">
                                    <StackPanel VerticalAlignment="Center" HorizontalAlignment="Left">
                                        <TextBlock Text="Molarity: " Foreground="{DynamicResource textBrush}"/>
                                        <StackPanel Orientation="Horizontal" Margin="0,0,0,10">
                                            <local:NumberTextBox x:Name="molarityNumberBox" Number="1"/>
                                            <TextBlock Text="mol/L" Foreground="{DynamicResource unitBrush}" VerticalAlignment="Center" Margin="3,0"/>
                                        </StackPanel>
                                        <TextBlock Text="Molar mass: " Foreground="{DynamicResource textBrush}"/>
                                        <StackPanel Orientation="Horizontal" Margin="0,0,0,10">
                                            <local:NumberTextBox x:Name="molarMassNumberBox" Number="10"/>
                                            <TextBlock Text="g/mol" Foreground="{DynamicResource unitBrush}" VerticalAlignment="Center" Margin="3,0"/>
                                        </StackPanel>
                                    </StackPanel>
                                    <Rectangle Width="35" Fill="{DynamicResource reversibleReactionBrush}" Margin="10,0"/>
                                    <StackPanel HorizontalAlignment="Left">
                                        <TextBlock Text="Mass concentration: " Foreground="{DynamicResource textBrush}"/>
                                        <StackPanel Orientation="Horizontal" Margin="0,0,0,10">
                                            <local:NumberTextBox x:Name="massConcNumberBox" Number="1"/>
                                            <TextBlock Text="%" Foreground="{DynamicResource unitBrush}" Margin="3,0"/>
                                        </StackPanel>
                                        <TextBlock Text="Density of solution: " Foreground="{DynamicResource textBrush}"/>
                                        <StackPanel Orientation="Horizontal" Margin="0,0,0,10">
                                            <local:NumberTextBox x:Name="densityNumberBox" Number="1"/>
                                            <TextBlock Text="g/mL" Foreground="{DynamicResource unitBrush}" Margin="3,0"/>
                                        </StackPanel>
                                    </StackPanel>
                                </StackPanel>
                                <local:LabToggleButton EnabledText="MOLARITY" DisabledText="CONCENTRATION" DisabledBorderBrush="#FF00FFC5" DisabledBackgroundBrush="#3300FFC5"/>
                            </StackPanel>
                        </StackPanel>
                    </Border>
                </StackPanel>
            </TabItem>
            <TabItem x:Name="startupSettingsPage" Foreground="#FFECECEC">
                <Grid Margin="15">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>
                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Top">
                        <Label Content="Laboratory loading on startup:" Background="{x:Null}" Foreground="#FFECECEC" FontFamily="{DynamicResource NevisFont}" FontSize="16"/>
                        <local:LabToggleButton x:Name="startSelection" BorderBrush="#FF8F8F8F" Background="#FF272727" BorderThickness="1,0" Width="200" VerticalAlignment="Center" Height="22" Margin="10,0,0,0"/>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal" Visibility="{Binding Enabled, Converter={StaticResource BooleanToVisibilityConverter}, ElementName=startSelection}" Grid.Row="1" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="30,7,0,0">
                        <TextBlock Text="Laboratory path:" VerticalAlignment="Center"/>
                        <Label x:Name="selectStartupLabel" Background="#FF323232" BorderBrush="#FF009722" BorderThickness="1" VerticalAlignment="Top" Foreground="#FFD8D8D8" MinHeight="20" Margin="10,0,0,0" MinWidth="150"/>
                    </StackPanel>

                </Grid>
            </TabItem>
            <TabItem x:Name="aboutPage" Foreground="{DynamicResource labBlueBrush}" FontSize="25" FontFamily="{DynamicResource TelegraficoFont}">
                <StackPanel Orientation="Vertical" Margin="0,5,0,0">
                    <StackPanel.Resources>
                        <Style TargetType="StackPanel">
                            <Setter Property="VerticalAlignment" Value="Top"/>
                            <Setter Property="HorizontalAlignment" Value="Center"/>
                            <Setter Property="Orientation" Value="Horizontal"/>
                        </Style>
                    </StackPanel.Resources>
                    <StackPanel>
                        <TextBlock TextWrapping="Wrap" Text="Version: " VerticalAlignment="Center"/>
                        <TextBlock x:Name="versionBlock" TextWrapping="Wrap" FontFamily="{DynamicResource NevisFont}"/>
                    </StackPanel>
                    <StackPanel Margin="0,5,0,0">
                        <TextBlock TextWrapping="Wrap" Text="Author: " VerticalAlignment="Center"/>
                        <TextBlock x:Name="authorBlock" TextWrapping="Wrap" FontFamily="{DynamicResource NevisFont}" Text="Dan Toskan"/>
                    </StackPanel>
                </StackPanel>
            </TabItem>


        </TabControl>
    </Grid>
</local:LabAssistantWindow>
