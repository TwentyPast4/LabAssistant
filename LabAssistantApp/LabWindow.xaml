                                <local:TableButton Tag="52"/>
                                <local:TableButton Tag="53"/>
                                <local:TableButton Tag="54"/>
                            </StackPanel>
                            <StackPanel Grid.Row="5" Orientation="Horizontal">
                                <local:TableButton Tag="55"/>
                                <local:TableButton Tag="56"/>
                                <local:TableButton Tag="Lanthanides"/>
                                <local:TableButton Tag="72"/>
                                <local:TableButton Tag="73"/>
                                <local:TableButton Tag="74"/>
                                <local:TableButton Tag="75"/>
