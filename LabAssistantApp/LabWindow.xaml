                            <Rectangle Grid.Row="7" Height="{Binding ActualHeight, ElementName=hydrogenButton, Mode=OneWay}"/>
                            <StackPanel x:Name="LanthanidesStackPanel" Grid.Row="8" Orientation="Horizontal" HorizontalAlignment="Center">
